{
    "name": "enigma-hunter",
    "version": "2.0.0",
    "description": "Jogo de investigação e mistério com IA",
    "main": "upload-game-data.js",
    "scripts": {
        "serve": "firebase emulators:start",
        "upload-data": "node upload-game-data.js --emulator",
        "upload-data:production": "node upload-game-data.js --production",
        "deploy": "firebase deploy",
        "deploy:hosting": "firebase deploy --only hosting",
