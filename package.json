        "deploy:firestore": "firebase deploy --only firestore:rules",
        "logs": "firebase functions:log"
    },
    "keywords": [
        "game",
        "mystery",
        "investigation",
        "ai",
        "firebase"
    ],
    "author": "Enigma Hunter Team",
    "license": "MIT",
    "dependencies": {
        "firebase-admin": "^12.0.0"
    },
    "devDependencies": {
        "eslint": "^9.38.0",
        "firebase-tools": "^13.0.0"
    }
}
